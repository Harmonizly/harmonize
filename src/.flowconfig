[ignore]
.*/node_modules/*
.*/build/*
.*/static/*
.*/config/*

[include]

[libs]

[options]
all=true
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
unsafe.enable_getters_and_setters=true
esproposal.decorators=ignore
esproposal.export_star_as=enable

module.name_mapper='^client\/\(.*\)$' -> '<PROJECT_ROOT>/client/\1'
module.name_mapper='^configuration\/\(.*\)$' -> '<PROJECT_ROOT>/../config/\1'
module.name_mapper='^server\/\(.*\)$' -> '<PROJECT_ROOT>/server/\1'
module.name_mapper='^static\/\(.*\)$' -> '<PROJECT_ROOT>/../static/\1'
module.name_mapper='^test\/\(.*\)$' -> '<PROJECT_ROOT>/../test/\1'
